  </div>

  <div class='field'>
    <%= form.label :registartion_date %>
    <%= form.date_field :registartion_date %>
  </div>

  <div class='field'>
    <%= form.label :start_date %>
    <%= form.date_field :start_date %>
  </div>

  <div class='field'>
    <%= form.label :end_date %>
    <%= form.date_field :end_date %>
  </div>

  <%= form.label :country %>
  <%# Location.all.each do |location| %>
  <%= form.collection_check_boxes :location_ids, Location.all, :id, :name %>
  <%#= location.name %>
  <%# end %>

  <div class='actions'>
