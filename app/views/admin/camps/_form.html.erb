<%= form_with(model: @camp, url: admin_camps_path) do |form| %>
  <% if camp.errors.any? %>
    <div id='error_explanation'>
      <h2><%= pluralize(camp.errors.count, 'error') %> prohibited this camp from being saved:</h2>
      <ul>
        <% camp.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class='field'>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div class='field'>
    <%= form.label :camp_type %>
    <%= form.text_field :camp_type %>
  </div>

  <div class='field'>
